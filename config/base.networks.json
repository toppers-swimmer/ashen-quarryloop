{
  "description": "Static configuration describing supported Base networks and RPC endpoints.",
  "networks": {
    "base-mainnet": {
      "name": "Base Mainnet",
      "chainId": 8453,
      "rpc": {
        "primary": "https://mainnet.base.org",
        "fallbacks": [
          "https://base.llamarpc.com",
          "https://rpc.ankr.com/base"
        ]
      },
      "explorer": {
        "name": "BaseScan",
        "url": "https://basescan.org"
      },
      "testnet": false
    },
    "base-sepolia": {
      "name": "Base Sepolia",
      "chainId": 84532,
      "rpc": {
        "primary": "https://sepolia.base.org",
        "fallbacks": [
          "https://base-sepolia-rpc.publicnode.com"
        ]
      },
      "explorer": {
        "name": "BaseScan Sepolia",
        "url": "https://sepolia.basescan.org"
      },
      "testnet": true
    }
  },
  "defaults": {
    "network": "base-mainnet",
    "rpcTimeoutMs": 10000
  }
}
